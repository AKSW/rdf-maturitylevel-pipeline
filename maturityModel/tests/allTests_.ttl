rutt:ontology-uri-slash-at-end
    a rut:ManualTestCase ;
    dcterms:description "All resources defined by an ontology should end their base URI with a slash" ;
    rut:appliesTo rut:Schema ;
    rut:generated rut:ManuallyGenerated ;
    rut:references rdf:type ;
    rut:source <http://www.w3.org/2000/01/rdf-schema#> ;
    rut:sparqlWhere """ {
        ?this a owl:Ontology .
        FILTER (
          isIRI(?this) && !strEnds(str(?this), "/")
        )
        bind("test" as ?message)
        bind(?this as ?resource)
         }""" ;
    rut:sparqlPrevalence """ SELECT (count(?this) AS ?total) WHERE {
                                 ?this a owl:Ontology .
                                 FILTER (
                                   isIRI(?this)
                                 )
          } """ ;

    rut:testCaseLogLevel
              rlog:ERROR .
